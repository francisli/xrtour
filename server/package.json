{
  "name": "xrtour-server",
  "version": "1.0.28",
  "private": true,
  "type": "module",
  "scripts": {
    "pp": "prettier --write --ignore-unknown .",
    "pp:changed": "prettier --write --ignore-unknown $(git diff --name-only --relative .; git ls-files -o --exclude-standard)",
    "lint": "eslint .",
    "lint:changed": "eslint $(git diff --name-only --relative .; git ls-files -o --exclude-standard)",
    "start": "nodemon -V --ignore ./client --ignore ./migrations --ignore ./public --ignore ./test ./bin/www",
    "test": "mocha --recursive"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.629.0",
    "@aws-sdk/client-transcribe": "^3.744.0",
    "@aws-sdk/cloudfront-signer": "^3.621.0",
    "@aws-sdk/s3-request-presigner": "^3.629.0",
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.1.0",
    "debug": "~4.3.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "email-templates": "^12.0.1",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "fs-extra": "^11.2.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "i18n": "^0.15.1",
    "inflection": "^3.0.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "mime-types": "^2.1.35",
    "mixpanel": "^0.18.0",
    "morgan": "~1.10.0",
    "mv": "^2.1.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "pm2": "^5.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.0",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "cross-env": "~7.0.3",
    "del-cli": "^5.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mocha": "^10.5.0",
    "foreman": "^3.0.1",
    "mocha": "^10.7.3",
    "nodemailer-mock": "^2.0.6",
    "sequelize-fixtures": "^1.2.0",
    "supertest": "^7.0.0",
    "supertest-session": "^5.0.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:mocha/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "import",
      "mocha"
    ],
    "rules": {
      "mocha/no-mocha-arrows": [
        "off"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__dirname"
          ],
          "allowAfterThis": true
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "Sequelize|queryInterface"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    },
    "ignorePatterns": [
      "build",
      "client",
      "node_modules"
    ]
  }
}
